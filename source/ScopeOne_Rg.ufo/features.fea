} frac_numr;

lookup frac_dnom {
	sub [fraction @DFLT_dnom] @DFLT_figPLnum' by @DFLT_dnom;
} frac_dnom;

lookup liga_dflt {
	sub f l by fl;
} liga_dflt;

lookup liga_i {
	sub f i by fi;
} liga_i;

#----------------------#
# Basic latin features #
#----------------------#

feature locl {
	script latn;
		language CAT exclude_dflt;
		lookup locl_CAT;
		language ROM exclude_dflt;
		lookup locl_ROM;
		language MOL exclude_dflt;
		lookup locl_ROM;
		language NLD exclude_dflt;
		sub Iacute J' lookup locl_NLD_ijacute;
		sub iacute j' lookup locl_NLD_ijacute;
		lookup locl_NLD_liga_acute;
} locl;
feature ccmp {
	lookup ccmp_decomposition {
		sub @DFLT_ccmp_decomp_top' lookup DFLT_dflt_ccmp_decomp_top @DFLT_ccmp_mark_below;
		sub @DFLT_ccmp_decomp_bottom' lookup DFLT_dflt_ccmp_decomp_bottom @DFLT_ccmp_mark_above;
	}ccmp_decomposition;
	lookup ccmp_softdotted {
		sub @DFLT_ccmp_softdotted' lookup DFLT_dflt_ccmp_softdotted @DFLT_ccmp_mark_below @DFLT_ccmp_mark_above ;
		sub @DFLT_ccmp_softdotted' lookup DFLT_dflt_ccmp_softdotted @DFLT_ccmp_mark_above;
	} ccmp_softdotted;
	lookup ccmp_mark_UC {
		sub @DFLT_UC @DFLT_mark_LC_top' by @DFLT_mark_UC_top;
		sub @DFLT_UC @DFLT_mark_LC_bottom' by @DFLT_mark_UC_bottom;
	} ccmp_mark_UC;
	lookup ccmp_ogonek_UC {
		sub [A E I] ogonekcomb' by ogonekcomb.cap;
		sub U ogonekcomb' by ogonekcomb.round.cap;
	} ccmp_ogonek_UC;
	lookup ccmp_mark_UC_2 {
		sub @DFLT_UC @DFLT_ccmp_mark_below_UC @DFLT_ccmp_mark_above' by @DFLT_ccmp_mark_above_UC;
		sub @DFLT_UC @DFLT_ccmp_mark_above_UC @DFLT_ccmp_mark_below' by @DFLT_ccmp_mark_below_UC;
	} ccmp_mark_UC_2;
	lookup ccmp_ogonek.round_LC {
		sub e ogonekcomb' by ogonekcomb.round;
	} ccmp_ogonek.round_LC;
	lookup ccmp_mark_top_right {
		sub [L d l] caroncomb' by commaaboverightcomb;
		sub t caroncomb' by commaaboverightcomb.short;
	} ccmp_mark_top_right;
	lookup ccmp_mark_ASC {
		sub @DFLT_ASC @DFLT_mark_LC_top' by @DFLT_mark_LC_asc;
	} ccmp_mark_ASC;
	lookup ccmp_mark_DSC {
		sub @DFLT_DSC @DFLT_mark_LC_bottom' by @DFLT_mark_LC_dsc;
	} ccmp_mark_DSC;
} ccmp;
feature frac {
	lookup frac_bar;
	lookup frac_numr;
	lookup frac_dnom;
} frac;
feature sups {
	sub @DFLT_figPLnum by @DFLT_sups;
} sups;
feature subs {
	sub @DFLT_figPLnum by @DFLT_subs;
} subs;
feature sinf {
	sub @DFLT_figPLnum by @DFLT_subs;
} sinf;
feature numr {
	sub @DFLT_figPLnum by @DFLT_numr;
} numr;
feature dnom {
	sub @DFLT_figPLnum by @DFLT_dnom;
} dnom;
feature lnum {
	sub @DFLT_figPOnum by @DFLT_figPLnum;
} lnum;
feature onum {
	sub @DFLT_figPLnum by @DFLT_figPOnum;
} onum;
feature pnum {
	sub @DFLT_figTLnum by @DFLT_figPLnum;
	sub @DFLT_figTOnum by @DFLT_figPOnum;
} pnum;
feature tnum {
	sub @DFLT_figPLnum by @DFLT_figTLnum;
	sub @DFLT_figPOnum by @DFLT_figTOnum;
} tnum;
feature ordn {
	sub [a o] by [ordfeminine ordmasculine] ;
} ordn;
feature case {
	sub @DFLT_case_base by @DFLT_case_alt ;
} case;
feature liga {
	lookup liga_dflt;
	lookup liga_i;
	script latn;
		language AZE exclude_dflt;
		lookup liga_dflt;
		language CRT exclude_dflt;
		lookup liga_dflt;
		language GAG exclude_dflt;
		lookup liga_dflt;
		language KAZ exclude_dflt;
		lookup liga_dflt;
		language TAT exclude_dflt;
		lookup liga_dflt;
		language TRK exclude_dflt;
		lookup liga_dflt;
} liga;



