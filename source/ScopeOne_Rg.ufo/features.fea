lookup DFLT_dflt_ccmp_softdotted {
	sub i by dotlessi;
	sub j by dotlessj;
} DFLT_dflt_ccmp_softdotted;

lookup locl_CAT {
	sub l periodcentered' l by periodcentered.loclCAT;
	sub L periodcentered' [L l] by periodcentered.loclCAT.cap;
} locl_CAT;

lookup locl_ROM {
	sub [Scedilla Tcedilla scedilla tcedilla] by [Scommabelow Tcommabelow scommabelow tcommabelow];
} locl_ROM;

lookup locl_NLD_ijacute {
	sub j by dotlessj acutecomb;
	sub J by J acutecomb.cap;
} locl_NLD_ijacute;

lookup locl_NLD_liga_acute {
	sub ij' acutecomb' by ijacute;
	sub IJ' acutecomb' by IJacute;
} locl_NLD_liga_acute;

lookup frac_bar {
	ignore sub slash' @DFLT_figPLnum slash;
	ignore sub slash @DFLT_figPLnum slash';
	ignore sub slash' @DFLT_figPLnum @DFLT_figPLnum slash;
	ignore sub slash @DFLT_figPLnum @DFLT_figPLnum slash';
	sub @DFLT_figPLnum [slash divisionslash]' @DFLT_figPLnum by fraction;
} frac_bar;

lookup frac_numr {
	sub @DFLT_figPLnum' fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
	sub @DFLT_figPLnum' @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum @DFLT_figPLnum fraction by @DFLT_numr;
